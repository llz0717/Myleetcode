class Solution:
    def shiftGrid(self, grid: List[List[int]], k: int) -> List[List[int]]:
        m,n=len(grid),len(grid[0])

        for _ in range(k):
            previous_element=grid[-1][-1]             
            for i in range(m):
                for j in range(n):
                    tomove=grid[i][j]
                    grid[i][j]=previous_element
                    previous_element=tomove
        return grid
